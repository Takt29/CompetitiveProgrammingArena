{
  "name": "competitive-programming-arena-frontend",
  "version": "0.1.0",
  "description": "frontend for competitive programming arena",
  "scripts": {
    "start": "NODE_ENV=development webpack serve",
    "build": "NODE_ENV=production webpack --mode production",
    "build:analyze": "NODE_ENV=production ANALYZE=1 webpack --mode production",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint --ext ts,tsx --ignore-path .gitignore --ignore-path .eslintignore .",
    "prettier": "prettier --ignore-path .gitignore --ignore-path .prettierignore --loglevel warn '**/*.{tsx,ts,jsx,js}'",
    "format": "npm run prettier -- --write && npm run lint -- --fix",
    "tsc": "tsc --noEmit",
    "cspell": "cspell lint --no-progress '**'"
  },
